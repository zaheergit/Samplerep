   "paths":{
      "/findata/orgId/{orgId}/templateVersionId/{templateVersionId}":{
         "get":{
            "tags":[
               "findata",
               "agorg"
            ],
            "summary":"AG Financial Data Services",
            "description":"Get financial data for an org by datasets",
            "operationId":"getTemplateData",
            "consumes":[
               "application/json",
               "application/xml"
            ],
            "produces":[
               "application/json",
               "application/xml"
            ],
            "parameters":[
               {
                  "name":"orgId",
                  "in":"path",
                  "required":true,
                  "type":"integer",
                  "format":"int64"
               },
               {
                  "name":"templateVersionId",
                  "in":"path",
                  "required":true,
                  "type":"integer",
                  "format":"int64"
               },
               {
                  "name":"setId",
                  "in":"query",
                  "required":false,
                  "type":"integer",
                  "format":"int64"
               },
               {
                  "name":"userId",
                  "in":"query",
                  "required":false,
                  "type":"integer",
                  "format":"int64"
               },
               {
                  "name":"practiceName",
                  "in":"query",
                  "required":false,
                  "type":"string"
               },
               {
                  "name":"datasetIds",
                  "in":"query",
                  "required":false,
                  "type":"string"
               },
               {
                  "name":"deVersionId",
                  "in":"query",
                  "required":false,
                  "type":"array",
                  "items":{
                     "type":"integer",
                     "format":"int64"
                  },
                  "collectionFormat":"multi"
               }
            ],
            "responses":{
               "200":{
                  "description":"successful operation",
                  "schema":{
                     "$ref":"#/definitions/Organization"
                  }
               }
            }
         }
      },
