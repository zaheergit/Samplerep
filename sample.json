      },
      "/findata/swagger/{filename}.{ext}":{
         "get":{
            "tags":[
               "findata",
               "agorg"
            ],
            "operationId":"swagger",
            "produces":[
               "application/octet-stream"
            ],
            "parameters":[
               {
                  "name":"filename",
                  "in":"path",
                  "required":true,
                  "type":"string"
               },
               {
                  "name":"ext",
                  "in":"path",
                  "required":true,
                  "type":"string",
                  "pattern":"json|yaml"
               }
            ],
            "responses":{
               "default":{
                  "description":"successful operation"
               }
            }
         }
      },
